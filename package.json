{
  "name": "ATPClass",
  "version": "0.0.0",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "build": "ng b --prod --base-href ./ --aot=false --build-optimizer=false",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ --watch && tsc --p electron --watch && electron .",
    "dist": "electron-builder --mac",
    "buildmain": "tsc --target es2015 --p electron",
    "buildinject": "tsc --target es5 --lib es2018,dom inject.ts",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "atpclass.vn",
    "productName": "ATP CLASS",
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "extraFiles": [],
    "files": [
      "electron/dist/main.js",
      "node_modules",
      "package.json"
    ],
    "asar": false,
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^12.0.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^5.0.0",
    "archiver": "^5.3.1",
    "auto-launch": "^5.0.5",
    "body-parser": "^1.19.0",
    "builder-util": "^22.11.8",
    "core-js": "^2.5.4",
    "electron-push-receiver": "^2.1.3",
    "express": "^4.17.1",
    "extract-zip": "^1.7.0",
    "firebase": "^7.11.0",
    "fs-extra": "^10.0.0",
    "i18n": "^0.15.1",
    "jquery": "^3.3.1",
    "karma-electron-launcher": "^0.3.0",
    "moment": "^2.24.0",
    "ngx-toastr": "^9.1.1",
    "node-2fa": "^1.1.2",
    "node-cron": "^2.0.3",
    "node-gyp": "^8.1.0",
    "node-machine-id": "^1.1.10",
    "popper": "^1.0.1",
    "popper.js": "^1.14.7",
    "proxy-check": "^1.0.8",
    "public-ip": "^4.0.1",
    "selenium-webdriver": "^3.6.0",
    "sinon-chai": "^3.7.0",
    "sqlite3": "^5.0.0",
    "sweetalert2": "^8.5.0",
    "system-commands": "^1.1.7",
    "toastr": "^2.1.4",
    "unzip-crx-3": "^0.2.0",
    "xlsx": "^0.14.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.7",
    "@angular/cli": "^7.0.7",
    "@angular/http": "^7.0.0",
    "@angular/language-service": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@types/electron": "^1.6.10",
    "@types/fs-extra": "^8.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "14.6.2",
    "@types/sqlite3": "^3.1.4",
    "chai": "^4.3.7",
    "core-js": "^2.5.4",
    "electron": "^13.1.6",
    "electron-builder": "^23.0.2",
    "electron-rebuild": "^3.2.9",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-electron": "^7.3.0",
    "karma-electron-launcher": "^0.3.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "sinon": "^15.0.0",
    "sinon-chai": "^3.7.0",
    "ts-md5": "^1.2.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.1.2"
  }
}
